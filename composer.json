{
    "name": "inspire/lucky",
    "description": "The Cms Inspire by Passiom on Laravel Framework",
    "keywords": [
        "inspire",
        "inspire cms",
        "inspire lucky"
    ],
    "homepage": "http://webnambo.com",
    "support": {
        "issues": "https://github.com/inspiredby/lucky/issues",
        "document": "http://inspire.readthedocs.io"
    },
    "authors": [
        {
            "name": "Trinh Quoc Hoa",
            "email": "webnambo@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "7.2",
        "fideloper/proxy": "^4.0",
        "hassankhan/config": "^1.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "mcamara/laravel-localization": "^1.3",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "mrabbani/laravel-module-manager": "^1.5",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Inspire\\Base\\": "modules/base/src",
            "Inspire\\Acl\\": "modules/acl/src",
            "Inspire\\Post\\": "modules/post/src",
            "Inspire\\Page\\": "modules/page/src",
            "Inspire\\Dashboard\\": "modules/dashboard/src",
            "Inspire\\User\\": "modules/user/src",
            "Inspire\\Theme\\": "modules/theme/src",
            "Inspire\\Menu\\": "modules/menu/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}